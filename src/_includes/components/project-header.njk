<header>
    <a class="back-home" href="/index.html">Home</a>
    <h1>
        <span><time>{{ gomoreRatings.year }}</time>{{ gomoreRatings.client }}</span>
        <span>{{ gomoreRatings.title }}</span>
    </h1>
    <h2>
        {% for line in gomoreRatings.headline %}
            {{ line }}{% if not loop.last %}<br>{% endif %}
        {% endfor %}
    </h2>

    <h3 class="skills">
        <span>Skills <span>u</span><span>s</span><span>e</span><span>d</span></span>
    </h3>

    <ul class="skills">
        {% for skill in gomoreRatings.skills %}
        <li>{{ skill }}</li>
        {% endfor %}
    </ul>

    <p class="standfirst">{{ gomoreRatings.description }}</p>
</header>